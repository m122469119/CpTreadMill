<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="bgColor" format="color" />
    <attr name="fgColor" format="color" />
    <attr name="percent" format="float" />
    <attr name="startAngle" format="float" />

    <declare-styleable name="ColorfulRingProgressView">
        <attr name="bgColor" />
        <attr name="fgColorStart" format="color" />
        <attr name="fgColorEnd" format="color" />
        <attr name="colorfulStrokeWidth" format="dimension" />
        <attr name="startTagImg" format="reference"/>
        <attr name="percent" />
        <attr name="startAngle" />
    </declare-styleable>


    <declare-styleable name="CarouselPicker">
        <attr name="items_visible" />
    </declare-styleable>

    <attr name="items_visible" format="enum">
        <enum name="three" value="3" />
        <enum name="five" value="5" />
        <enum name="seven" value="7" />
    </attr>
</resources>